{
  "name": "parser-multipart",
  "version": "1.0.12",
  "description": "Multipart form data parser for browser and node",
  "keywords": [
    "batch",
    "data",
    "fetch",
    "form",
    "form-data",
    "multipart",
    "multipart-parser",
    "parse",
    "parse-multipart-data",
    "parser",
    "parts",
    "typescript"
  ],
  "homepage": "https://github.com/kmalakoff/parser-multipart#README.md",
  "bugs": {
    "url": "https://github.com/kmalakoff/parser-multipart/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/parser-multipart.git"
  },
  "license": "MIT",
  "author": {
    "name": "Kevin Malakoff",
    "email": "kmalakoff@gmail.com",
    "url": "https://github.com/kmalakoff"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.cjs",
  "unpkg": "dist/umd/parser-multipart.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "clean": "node -e \"const dir = require('path').join(require('homedir-polyfill')(), '.im'); try {require('fs').rmdirSync(dir, {recursive: true})} catch(err) {}\"",
    "deploy": "tsds deploy",
    "docs": "tsds docs",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts && tsds test:browser --config wtr.config.mjs",
    "test:engines": "nvu engines tsds test:node --no-timeouts && tsds test:browser --config wtr.config.mjs",
    "version": "tsds version"
  },
  "dependencies": {
    "newline-iterator": "^0.3.19"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.5",
    "@web/dev-server-import-maps": "^0.2.1",
    "multi-data": "^0.2.15",
    "pinkie-promise": "^2.0.1",
    "tsds-web-test-runner": "^1.13.6"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts",
    "targets": [
      "cjs",
      "esm",
      "umd"
    ],
    "globals": {
      "newline-iterator": "newlineIterator"
    }
  }
}
